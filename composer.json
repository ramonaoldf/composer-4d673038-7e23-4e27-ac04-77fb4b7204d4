{
    "name": "presttec\/laravel-browser-kit-testing",
    "description": "Provides backwards compatibility for BrowserKit testing in Laravel 5.4.",
    "keywords": [
        "laravel",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "presttec\/illuminate-support": "^5.6",
        "presttec\/mockery": "^1.0",
        "presttec\/phpunit": "^7.0",
        "presttec\/css-selector": "~4.0",
        "presttec\/dom-crawler": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BrowserKitTesting\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/browser-kit-testing": "*"
    },
    "conflicts": {
        "laravel\/browser-kit-testing": "*"
    },
    "require-dev": []
}