{
    "name": "presttec\/laravel-browser-kit-testing",
    "description": "Provides backwards compatibility for BrowserKit testing in the latest Laravel release.",
    "keywords": [
        "laravel",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ramon Oliveira",
            "email": "ramon@presttec.com.br"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-dom": "*",
        "ext-json": "*",
        "presttec\/illuminate-contracts": "^7.0|^8.0",
        "presttec\/illuminate-database": "^7.0|^8.0",
        "presttec\/illuminate-http": "^7.0|^8.0",
        "presttec\/illuminate-support": "^7.0|^8.0",
        "presttec\/illuminate-testing": "^7.0|^8.0",
        "presttec\/mockery": "^1.0",
        "presttec\/phpunit": "^8.5|^9.0",
        "presttec\/console": "^5.0",
        "presttec\/css-selector": "^5.0",
        "presttec\/dom-crawler": "^5.0",
        "presttec\/http-foundation": "^5.0",
        "presttec\/http-kernel": "^5.0"
    },
    "require-dev": {
        "presttec\/laravel-framework": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BrowserKitTesting\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\BrowserKitTesting\\Tests\\": "tests\/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replaces": {
        "laravel\/browser-kit-testing": "*"
    },
    "conflicts": {
        "laravel\/browser-kit-testing": "*"
    }
}